#!/usr/bin/env bash                                                                                                                                                                                                                                                             

## run script for different core numbers                                                                                                                                                                                                                                        
for h in 0.1 0.05 0.001 0.005
do
    ./a.out $h
done

## generate timing plot                                                                                                                                                                                                                                                         
Rscript p2b.R



